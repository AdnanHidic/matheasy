<Window x:Class="MathEasy.src.Editing.Controls.UploadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UploadWindow" Height="125" Width="374" Icon="/MathEasy;component/images/AppIcon.ico" ShowInTaskbar="False" KeyUp="Window_KeyUp" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <Label Content="URL:" Height="28" HorizontalAlignment="Left" Margin="12,21,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="26" HorizontalAlignment="Left" Margin="52,23,0,0" Name="tb_url" VerticalAlignment="Top" Width="288" FontSize="13" IsReadOnly="True" />
        <Label Content="URL has been placed in your clipboard as well." Height="28" HorizontalAlignment="Left" Margin="12,58,0,0" Name="label2" VerticalAlignment="Top" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="277,58,0,0" Name="b_OK" VerticalAlignment="Top" Width="63" Click="b_OK_Click" />
    </Grid>
</Window>
